[user]
    name = Your Name
    email = your.email@example.com

[core]
    editor = code --wait
    autocrlf = input

[init]
    defaultBranch = main

[pull]
    rebase = false

[push]
    default = current

[alias]
    unstage = reset HEAD --
    last = log -1 HEAD
    visual = log --graph --oneline --all

[color]
    ui = auto